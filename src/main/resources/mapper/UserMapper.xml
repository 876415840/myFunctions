<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alan.mapper.UserMapper" >
  <resultMap id="BaseResultMap" type="com.alan.model.User" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="name" property="name" jdbcType="VARCHAR" />
    <result column="age" property="age" jdbcType="INTEGER" />
  </resultMap>
  <insert id="insert" parameterType="com.alan.model.User" >
    insert into user (id, name, age)
    values (#{id}, #{name}, #{age})
  </insert>
  <select id="findUserById" resultMap="BaseResultMap" parameterType="java.lang.Integer">
      select * from user where id=#{id}
  </select>

</mapper>